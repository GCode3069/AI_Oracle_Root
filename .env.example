# LLM Battle Royale Environment Variables
# Copy this file to .env and populate with your actual API keys
# NEVER commit .env to version control

# ElevenLabs API key for voiceover generation
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Pexels API key for stock footage
PEXELS_API_KEY=your_pexels_api_key_here

# Runway API key for video generation (if using Runway)
RUNWAY_API_KEY=your_runway_api_key_here

# YouTube OAuth client secrets file path (for upload)
YOUTUBE_CLIENT_SECRETS=path/to/client_secrets.json

# Optional: Pollo API key for additional video generation
POLLO_API_KEY=your_pollo_api_key_here

# Optional: Stability API key for image generation
STABILITY_API_KEY=your_stability_api_key_here
