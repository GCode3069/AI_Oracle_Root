# Environment Variables Template for LLM Battle Royale
# Copy this file to .env and populate with your actual API keys
# NEVER commit .env to version control!

# Required API Keys
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
PEXELS_API_KEY=your_pexels_api_key_here

# Optional API Keys (for enhanced video generation)
RUNWAY_API_KEY=your_runway_api_key_here
POLLO_API_KEY=your_pollo_api_key_here
STABILITY_API_KEY=your_stability_api_key_here

# YouTube Upload (OAuth 2.0 client secrets file path)
YOUTUBE_CLIENT_SECRETS=./credentials/client_secrets.json

# Production Settings
PRODUCTION_MODE=false
DRY_RUN=true
