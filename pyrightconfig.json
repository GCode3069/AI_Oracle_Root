{
  "include": [
    "**/*.py"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.*",
    "**/output",
    "**/outputs",
    "**/videos",
    "**/audio",
    "**/images",
    "**/temp",
    "**/tmp",
    "**/cache",
    "**/assets",
    "**/generated_videos",
    "**/uploaded",
    "**/batch_upload",
    "**/channels/**/videos",
    "**/channels/**/audio",
    "**/channels/**/scripts",
    "**/channels/**/analytics",
    "**/channels/**/assets",
    "**/channels/**/uploaded",
    "**/abraham_horror/videos",
    "**/abraham_horror/audio",
    "**/abraham_horror/images",
    "**/abraham_horror/batch_upload",
    "**/abraham_horror/temp",
    "**/tiktok/generated_videos",
    "**/tiktok/upload_queue",
    "**/tiktok/analytics",
    "**/.git",
    "**/venv",
    "**/env",
    "**/.venv",
    "**/Wav2Lip",
    "**/checkpoints"
  ],
  "ignore": [
    "**/*.mp4",
    "**/*.mp3",
    "**/*.wav",
    "**/*.avi",
    "**/*.mov",
    "**/*.mkv",
    "**/*.jpg",
    "**/*.jpeg",
    "**/*.png",
    "**/*.gif",
    "**/*.webp",
    "**/*.json",
    "**/*.txt",
    "**/*.md",
    "**/*.bat",
    "**/*.ps1",
    "**/*.sh",
    "**/*.html",
    "**/*.css",
    "**/*.js",
    "**/*.ipynb"
  ],
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.9",
  "typeCheckingMode": "basic"
}
